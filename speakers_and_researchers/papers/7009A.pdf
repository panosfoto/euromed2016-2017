    <script src="http://www.watchuseek.com/wp-content/themes/watchuseek/js/foundation.min.js"></script>
    <script src="http://www.watchuseek.com/wp-content/themes/watchuseek/js/foundation/foundation.equalizer.js"></script> 
    
    <script>
      $(document).foundation({
		  equalizer : {
			// Specify if Equalizer should make elements equal height once they become stacked.
			equalize_on_stack: true
		  }
	  });
	  (function ($, window) {
        $(document).ready(function(){
			// Search function	  		
			$("#topSearch .search-icon").click(function(){
				$("#topSearch > div").toggleClass("search-open");
				if ($("#topSearch > div").hasClass("search-open")) $("#topSearch .gsc-input").focus();
			});
			
			/* Copy menu for mobile */
			$("#navBar #menu-top-menu").clone().appendTo("#socialNav .mobileMenu-icon div");
			
			//make Related stories same height
			var relatedUL=$("ul.related_post");
			if (relatedUL.length){
				relatedUL.attr('data-equalizer','');
				relatedUL.children('li').attr('data-equalizer-watch','');
				$(document).foundation('equalizer','reflow');
			}
			
			/* Nav scroll */			
			var NavBar=$('#socialNav');
			var NavBarofset=NavBar.height();	
			var didScroll;
			var lastScrollTop = 0;
			var delta = 5;
			var navbarHeight = NavBar.height();
			//wrap navBar into div with set height
			NavBar.wrap('<div id="setHeightPleaceholder" style="height:'+NavBarofset+'px"></div>');
			if (typeof startStick != 'undefined' && $(window).scrollTop()>=startStick) $(window).trigger("scroll");
			$(window).scroll(function() {				
				  if (typeof startStick == 'undefined') 
				  									//var startStick=$("#navBar").offset().top+parseInt($("#navBar").height());	
							var startStick=1;
										  var scr = $(window).scrollTop();				
				  if (scr >= startStick) {
					  if (!NavBar.hasClass("fixedNavbar")) NavBar.addClass("fixedNavbar");//NavBar.hide().delay(200).addClass("fixedNavbar").fadeIn(600);		 					  
				  } else {					  
					  NavBar.stop( true, true ).removeClass("fixedNavbar").removeClass("fixedNavbarBottom");					  
				  }	
				  didScroll = true;
			});
			
						function hasScrolled() {
				var st = $(this).scrollTop();
				
				// Make sure they scroll more than delta
				if(Math.abs(lastScrollTop - st) <= delta)
					return;
				
				// If they scrolled down and are past the navbar, add class .nav-up.
				// This is necessary so you never see what is "behind" the navbar.
				if (lastScrollTop!=0 && st > lastScrollTop && st > navbarHeight){
					// Scroll Down
					$('#socialNav').removeClass('nav-down').addClass('nav-up');
				} else {
					// Scroll Up
					if(st + $(window).height() < $(document).height()) {
						$('#socialNav').removeClass('nav-up').addClass('nav-down');
					}
				}
				
				lastScrollTop = st;
			}
			
			setInterval(function() {
				if (didScroll && $(window).width()<641) {
					hasScrolled();
					didScroll = false;
				}
			}, 250);
						/* Window resize listener */
			$(window).resize(function () {
				// Update start stick variable
									//var startStick=$("#navBar").offset().top+parseInt($("#navBar").height());	
					var startStick=1;
								//Update nav placeholder height
				NavBarofset=NavBar.height();
				$('#setHeightPleaceholder').height(NavBarofset);
			});
			
			//Hamburger click or mouseover
			$(".mobileMenu-icon").click(function () {
				$(this).toggleClass("hovertouch-hover");
		    });
						
									
			/* Nav for tablets and mobile */
			$("#header ul .hovertouch").click(function(e){
				/* Check if not touchscreen and smaller than desktop */										   
				if (!('ontouchstart' in document.documentElement) && ($(window).width()>=1024)){  
					//desktop resized to mobile view. No touchscreen.
					$(this).addClass('hovertouch-hover');
					if ($("#header ul .hovertouch-hover").length<=1) $(this).find('.topnav-dd').slideDown(400,function() {
							
						  });
					else $this.find('.topnav-dd').show(0,function() {
								
							});
					return true;
				}			
				/* if mobile */
				if ($(window).width()<1024){					
					// do onclick menu
					if ($(e.target).hasClass("main-menu-link") && $(e.target).parent().hasClass('menu-item-has-children')) {
						$(this).toggleClass('hovertouch-hover');
						return false;
					} else {
						return true;
					}
				}
				
			});
			/* Hover events for non-touch screens - tablets and desktop */			
			if (!('ontouchstart' in document.documentElement)) {
				var $dropdowns = $('#header ul .hovertouch');				
				$dropdowns
				.on('mouseover', function(e){ 
					if ($(window).width()>1023 && !$(this).hasClass('hovertouch-hover')){					  
					var $this = $(this);
					if ($this.prop('hoverTimeout')) $this.prop('hoverTimeout', clearTimeout($this.prop('hoverTimeout')));
					
					$this.prop('hoverIntent', setTimeout(function(){
						$this.addClass('hovertouch-hover');
						if ($("#header ul .hovertouch-hover").length<=1) $this.find('.topnav-dd').slideDown(400,function() {
							
						  });
						else $this.find('.topnav-dd').show(0,function() {
								
							});
					}, 300));
					}
				})
				.on('mouseleave', function(){
					if ($(window).width()>1023){					   
					var $this = $(this);
					if ($this.prop('hoverIntent')) $this.prop('hoverIntent', clearTimeout($this.prop('hoverIntent')));          
		
					$this.prop('hoverTimeout', setTimeout(function(){
						$this.removeClass('hovertouch-hover');
						$this.find('.topnav-dd').stop(true,true).attr("style",""); 
					}, 500));
					}
				});				
			}
			
			/* Get facebook share count */
			var fbs = $('[data-facebook-share-count-url]');
	
			if (fbs.length) {
				// Collect all URLs and map it back to matching elements
				var urls = [];
				var fbData = {};
	
				fbs.each(function(k, elem) {
					var url = $(elem).data('facebook-share-count-url');
					if (!fbData[url]) {
						fbData[url] = [];
					}
					fbData[url].push(elem);
					urls.push(url);
				});
	
				$.each(fbData, function(url, elems) {
					$.ajax({
						url: "http://graph.facebook.com/v2.7/",
						data: { id: url, fields: "share" },
						success: function(data) {
							if (typeof(data.share) != 'undefined' && data.share.share_count != 0) {
								var totalShares = data.share.share_count; // using total count (=share_count+like_count)
								$(elems).html("("+totalShares+")");
							}
						}
					});     
				});
			}
			
			//Get facebook comment count
			var fbc=$('[data-facebook-comment-count-url]');
			
			if (fbc.length) {
				// Collect all URLs and map it back to matching elements
				var urls = [];
				var fbData = {};
	
				fbc.each(function(k, elem) {
					var url = $(elem).data('facebook-comment-count-url');
					if (!fbData[url]) {
						fbData[url] = [];
					}
					fbData[url].push(elem);
					urls.push(url);
				});
	
				$.each(fbData, function(url, elems) {
					$.ajax({
						url: "https://graph.facebook.com/v2.7/?id="+url+"&fields=share{comment_count}&format=json",
						success: function(data) {
							if (data.share.comment_count) {
								var commentCount = parseInt(data.share.comment_count); 
								if (commentCount>0) $(elems).html("("+commentCount+")");
							}
						}
					});     
				});
			}
			
			// Don't submit newsletter form if email is empty
			$(".subscribe-form").submit(function(event){
				if ($(this).find("input[name='form[email]']").val()=='') {
					event.preventDefault();
					return false;
				}
			});
			
		}); // document ready
   	  })(jQuery, window);
	  
    </script>
        
    

		<script>


			// If the Date.now() function doesn't exist, then create it
			if (!Date.now) {
			    Date.now = function() { return new Date().getTime(); }
			}


			////////// Overlay boxes Start //////////////////

			function createCookie(name,value,days) {
				if (days) {
					var date = new Date();
					date.setTime(date.getTime()+(days*24*60*60*1000));
					var expires = "; expires="+date.toGMTString();
				}
				else
					var expires = "";
				document.cookie = name+"="+value+expires+"; path=/";
			}

			function readCookie(name) {
				var nameEQ = name + "=";
				var ca = document.cookie.split(';');
				for(var i=0;i < ca.length;i++) {
					var c = ca[i];
					while (c.charAt(0)==' ') c = c.substring(1,c.length);
					if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
				}
				return null;
			}

			function eraseCookie(name) {
			    createCookie(name,"",-1);
			}


			(function ($, window) {

				//Cookie is set using following format: "vsCookieValueArray[i];TimeStamp"
				// Each popup window

				// Index URL defaults to the root of the domain. If that is not the case, if it's something like http://domain.com/articles is the root
				// of the site, then please change the indexUrl below

				if ( $(location).attr('pathname') != '/' ){ // Show only if not on the index page

					var vsAllSetCookieTSArray = [];
					var vsAllSetCookieValueArray = [];
					var dontShowAnyPopups = 0;  // 0/1 Show or not to show the popup

					//First collect all data about shown overlays
					$("#vspopup-all-overlays .vsoverlay-item").each(function(){
						var vsOverlay = $(this);

						var vsCookieExpDays = vsOverlay.data("cookie-exp-days"); // How many days to set the cookie for
						var vsCookieName = vsOverlay.data("cookie-name"); // Cookie name for this popup
						var vsCookieValue = vsOverlay.data("cookie-value"); // If the CookieName = (this value.timestamp), then that means it's set!


						if (readCookie(vsCookieName) != null){ // if cookie exists | no matter what the situation in here is, no popup will be shown

							var vsSetCookie = readCookie(vsCookieName).split(".");
							var vsSetCookieValue = vsSetCookie[0];
							var vsSetCookieTS = vsSetCookie[1];

							if (vsSetCookieTS == null || vsSetCookieTS == "") { // if timestamp is not set but cookie is set

								var vsCookieValueStr = vsSetCookieValue + "." + Date.now().toString(); // New value to be set
								createCookie(vsCookieName, vsCookieValueStr, vsCookieExpDays); // We set the proper value so next time we loop, we will know when it was last run

								// Just to be consistent, although we won't reach to the second loop, to keep
								vsAllSetCookieTSArray.push(vsSetCookieTS); //save element's timestamp, so we know it has been shown more than 24 hours ago
								vsAllSetCookieValueArray.push(vsSetCookieValue);

								dontShowAnyPopups = 1; // We don't know when cookie was set, so let's just wait 24 hours before showing any popups
								return false;

							} else { //timestamp is set

								// It was set less than 24 hours ago | We are not going to show any popups
								if ((parseInt(vsSetCookieTS) + 60 * 60 * 24 * 1000) > Date.now()){
									dontShowAnyPopups = 1; // dont show any popups
									return false;
								} else { // This popup can not be displayed. Cookies Set. Cookies Set and 24 hours ago.
									vsAllSetCookieTSArray.push(vsSetCookieTS); //save element's timestamp, so we know it has been shown more than 24 hours ago
									vsAllSetCookieValueArray.push(vsSetCookieValue);
								}
							}
						} else { // Cookie does not exist. Good sign. We can show this.
							vsAllSetCookieTSArray.push(""); // Save an empty string, so we know that this popup has not been shown
							vsAllSetCookieValueArray.push(vsSetCookieValue);
						}

					});


					if (dontShowAnyPopups == 0) { // if no popus have been seen in the last 24 hours

						// need to go through all popups again even if cookies are set - what if cookie value is different
						var popupCounter = 0;
						$("#vspopup-all-overlays .vsoverlay-item").each(function(){

							var vsOverlay = $(this);
							var vsCookieExpDays = vsOverlay.data("cookie-exp-days");
							var vsCookieName = vsOverlay.data("cookie-name");
							var vsCookieValue = vsOverlay.data("cookie-value");


							// This popup has not been shown or cookie value is different (Not Sure why this different case is there)
							if (vsAllSetCookieTSArray[popupCounter] == "" || vsCookieValue != vsAllSetCookieValueArray[popupCounter]) {
								window.setTimeout(function() {
									vsOverlay.fadeIn("slow");
								  }, 20000);
								return false; // show it and exit the loop
							}

							popupCounter++;
						}); // end $("#vspopup-all-overlays .vsoverlay-item").each
					}

				} // end if not a homepage


				// Close buttons of Overlays
				$("#vspopup-all-overlays .vsoverlay-item").delegate(".close-vspopup", "click", function(){

					var vsOverlay = $(this).closest(".vsoverlay-item");
					overlayCloser(vsOverlay, 0);

					return false;
				});

				// Terms of service
				$(".restrictions-label").click(function(event) {
					event.preventDefault();
					$(this).parents("#vspopup-overlay").find(".restrictions-msg, #vs-terms-close, .close-vspopup").toggleClass('opened');
				});

				// Close the terms of service
				$("#vspopup-overlay #vspopup-container #vs-terms-close").click(function(event) {
					event.preventDefault();
					$(this).parents("#vspopup-overlay").find(".restrictions-msg, #vs-terms-close, .close-vspopup").toggleClass('opened');
				});

				// Already liked. Means we should never show this popup ever again
				$("#vspopup-all-overlays .vsoverlay-item").delegate(".already-liked", "click", function(){

					var vsOverlay = $(this).closest(".vsoverlay-item");
					vsOverlay.data("cookie-exp-days", 9999); // Set to 9999 days expiry for the cookie
					overlayCloser(vsOverlay, 0);

					return false;
				});

				// function per overlay
				$("#vspopup-all-overlays form").submit(function(){

					var vsOverlay = $(this).closest(".vsoverlay-item");

					overlayCloser(vsOverlay);

				});

				// The helper to closing an overlay popup. Parameter overlay jQuery object
				function overlayCloser($overlay, delayClose)
				{
					var vsCookieExpDays = $overlay.data("cookie-exp-days");
					var vsCookieName = $overlay.data("cookie-name");
					var vsCookieValue = $overlay.data("cookie-value");
					delayClose = (typeof(delayClose) == "undefined") ? 0 : delayClose; // Set the delay before closing the overlay

					$overlay.delay(delayClose).fadeOut("slow");

					// Set the cookie with current timestamp for this overlay
					var vsCookieValueStr = vsCookieValue + "." + Date.now().toString();
					createCookie(vsCookieName,vsCookieValueStr,vsCookieExpDays); //set cookie
				}

			})(jQuery, window);
		</script>
